{
  "name": "vue-nuxt-passport-bulma-express-boiler",
  "version": "1.0.2",
  "description": "A vue/nuxtjs/passport/bulma boilerplate for express.",
  "author": "Scharkee",
  "scripts": {
    "dev": "node src/server.js",
    "build": "nuxt build",
    "start": "NODE_ENV=production node src/server.js",
    "launch": "nuxt build && npm run start",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "ava --serial --verbose",
    "setup": "node setup.js",
    "config": "npm run setup",
    "setup:headless": "node setup.js --headless"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "async": "^3.1.1",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "flash": "^1.1.0",
    "greenlock-express": "^4.0.3",
    "helmet": "^3.21.2",
    "le-challenge-fs": "^2.0.9",
    "le-store-certbot": "^2.2.3",
    "nedb": "^1.8.0",
    "nedb-session-store": "^1.1.2",
    "nuxt": "^2.11.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "serve-favicon": "^2.5.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "ava": "^3.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "fs-extra": "^8.1.0",
    "prompt-sync": "^4.2.0"
  },
  "prettier": {},
  "ava": {
    "files": [
      "test/**/*"
    ]
  }
}
